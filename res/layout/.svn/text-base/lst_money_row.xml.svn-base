<?xml version="1.0" encoding="UTF-8"?>  
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  
    android:layout_width="fill_parent" android:layout_height="?android:attr/listPreferredItemHeight"  
    android:padding="6dip">
    <ImageView android:id="@+id/lsticon"
        android:layout_width="wrap_content"  
        android:layout_height="fill_parent" android:layout_marginRight="6dip"  
        android:src="@drawable/blank_no_width" />  
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  
    	android:layout_width="0dip" 
    	android:layout_height="fill_parent"
    	android:layout_weight="1"
    	android:orientation="vertical"  
    	>
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  
			    	android:layout_width="fill_parent" 
			    	android:layout_height="0dip"
			    	android:orientation="horizontal"
			    	android:textSize="18dip"
			    	android:layout_weight="1"  
			    	>     	
	    	<TextView android:id="@+id/toptext" android:layout_width="0dip"  
	            android:layout_height="fill_parent" android:layout_weight="1"
	            android:layout_gravity="center_vertical" 
	    	/>
		   	<TextView android:id="@+id/toptextright" 
		   		android:layout_width="wrap_content"  
		           android:layout_height="fill_parent"
		           android:textSize="16dip"
		   	/>	    	
    	</LinearLayout>
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  
	    	android:layout_width="fill_parent" 
	    	android:layout_height="wrap_content"
	    	android:orientation="horizontal"  
	    	>    	
	    	<TextView android:id="@+id/bottomtext1cap"
	    		android:textSize="12dip" 
	    		android:textColor="@color/lightblue"
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content" 
	    	/>
	    	<TextView android:id="@+id/bottomtext1"
	    		android:textSize="12dip" 
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content"
	            android:layout_marginRight="6dip"
	    	/>
	    	<TextView android:id="@+id/bottomtext3cap"
	    		android:textSize="12dip" 
	    		android:textColor="@color/lightblue"
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content" 
	    	/>
	    	<TextView android:id="@+id/bottomtext3"
	    		android:textSize="12dip" 
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content"
	    	/>
	    </LinearLayout>
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  
	    	android:layout_width="fill_parent" 
	    	android:layout_height="wrap_content"
	    	android:orientation="horizontal"  
	    	>	    	
	    	<TextView android:id="@+id/bottomtext2cap"
	    		android:textSize="12dip" 
	    		android:textColor="@color/lightblue"
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content" 
	    	/>
	    	<TextView android:id="@+id/bottomtext2"
	    		android:textSize="12dip" 
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content"
	            android:layout_marginRight="6dip" 
	    	/>
	    	<TextView android:id="@+id/bottomtext4cap"
	    		android:textSize="12dip" 
	    		android:textColor="@color/lightblue"
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content" 
	    	/>
	    	<TextView android:id="@+id/bottomtext4"
	    		android:textSize="12dip" 
	    		android:layout_width="wrap_content"  
	            android:layout_height="wrap_content"
	    	/>
    	</LinearLayout>
    </LinearLayout>
</LinearLayout>