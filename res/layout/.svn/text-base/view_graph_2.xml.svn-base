<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
	<TableLayout
		android:background="@drawable/gradi_silver"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:stretchColumns="1"
	    >
	    <TableRow>
	    
	    <TextView android:text="月" android:layout_marginRight="5px"></TextView>
		<Spinner android:id="@+id/month_graph"
		        android:layout_width="fill_parent"
		        android:layout_height="wrap_content" />
		</TableRow>
	    <TableRow>
	   	<CheckBox android:id="@+id/chkExpVal"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_span="2"
	        android:text="期待値を表示する" />
	   	</TableRow> 		
	</TableLayout>
	<WebView
		android:id="@+id/wvViewGraphTest"
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:layout_weight="1.0"
	 >
	</WebView>
	
<LinearLayout
	android:id="@+id/graph_bottomBar"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/gradi_silver"
/>	

</LinearLayout>
